<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/all"
    android:layout_height="match_parent"
     >

    <RelativeLayout
        android:id="@+id/familyzone_feed_track"
        android:layout_width="50.0dip"
        android:layout_marginLeft="10dip"
        android:layout_height="65.0dip" >

        <com.example.ifamily.mywidget.RoundImageView
            android:id="@+id/familyzone_sch_head"
            android:layout_width="50dp"
            android:layout_height="50dp" />

        <com.example.ifamily.mywidget.TextViewM
            android:id="@+id/familyzone_feed_authorname"
            style="@style/author_name"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
           android:layout_below="@id/familyzone_sch_head"
 />
    </RelativeLayout>

    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@id/familyzone_feed_track"
        layout="@layout/familyzone_sch_beforitem" />

</RelativeLayout>